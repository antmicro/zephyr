menuconfig FPGA
	bool "FPGA Drivers"
	help
	  Enable support for FPGA drivers.

if FPGA

module = fpga

source "subsys/logging/Kconfig.template.log_config"

config FPGA_SHELL
	bool "Enable FPGA Shell"
	depends on SHELL && FPGA
	help
	  Enable FPGA Shell for testing.

source "drivers/fpga/Kconfig.eos_s3"

endif
